<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="ConsoleJsonViewer.uss" />
    <uie:Toolbar style="border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; margin-left: 2px; margin-right: 0; margin-top: 1px; margin-bottom: 1px; border-left-color: rgb(94, 91, 91); border-right-color: rgb(94, 91, 91); border-top-color: rgb(94, 91, 91); border-bottom-color: rgb(94, 91, 91); padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;">
        <uie:ToolbarToggle focusable="false" label="Link Console" value="true" name="LinkConsole" style="flex-shrink: 0; flex-grow: 0; position: relative; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
        <uie:ToolbarToggle focusable="false" label="ToJson" value="true" name="ConvertJson" style="margin-left: -1px; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
        <uie:ToolbarToggle focusable="false" label="AutoCheck" value="true" name="AutoCheck" style="margin-left: -1px; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
