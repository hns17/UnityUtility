            <ui:Label display-tooltip-when-elided="true" name="StateText" style="margin-top: 2px; margin-right: 10px;" />
        </ui:VisualElement>
    </uie:Toolbar>
    <ui:Foldout text="Viewer" style="border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; border-left-color: rgb(94, 91, 91); border-right-color: rgb(94, 91, 91); border-top-color: rgb(94, 91, 91); border-bottom-color: rgb(94, 91, 91); padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; margin-left: 1px; margin-right: 1px; margin-top: 1px; margin-bottom: 1px;">
        <ui:ScrollView mode="Vertical">
            <ui:TextField picking-mode="Ignore" value="filler text" readonly="false" multiline="true" style="flex-grow: 0; flex-shrink: 1; flex-direction: column; padding-left: 1px; padding-right: 1px; padding-top: 1px; padding-bottom: 1px; min-height: 150px; white-space: normal;" />
        </ui:ScrollView>
    </ui:Foldout>
